// Place your settings in this file to overwrite default and user settings.
{
    "editor.wordWrap": "bounded",
    "latex-workshop.latex.toolchain": [
        {
            "command": "latexdaemon",
            "args": [
                "-watch=no",
                "-exit",
                "-aux-directory=TexAux",
                "-afterjob=bibtex",
                "-custom_args=\"-synctex=1 -enable-installer\"",
                "%DOC%.tex"
            ]
        }
    ],
    "latex-workshop.latex.additionalBib": [
        "ulctrav.bib"

    ],
    "latex-workshop.chktex.enabled": true,
    "cSpell.words": [
        "bisimilar",
        "bisimilarity",
        "bisimulated",
        "bisimulation",
        "blum's",
        "daniil's",
        "injective",
        "ker's",
        "lloc",
        "moivre's",
        "reduct",
        "regnier",
        "subcase",
        "subterms",
        "surjective",
        "synctex"
    ],
    "cSpell.ignoreRegExpList": [
        "\\\\\\w*(\\[.*?\\])?(\\{.*?\\})?",
        "\\$.+?\\$"
      ]
}