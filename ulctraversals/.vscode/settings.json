// Place your settings in this file to overwrite default and user settings.
{
    "editor.wordWrap": "bounded",
    "latex-workshop.latex.toolchain": [
        {
            "command": "latexdaemon",
            "args": [
                "-watch=no",
                "-exit",
                "-aux-directory=TexAux",
                "-afterjob=bibtex",
                "-custom_args=\"-synctex=1 -enable-installer\"",
                "%DOC%.tex"
            ]
        }
       /*, {
            "command" : "bibtex",
            "args":[
                "%DOCFILE%"
            ]
        }*/
    ],
    "latex-workshop.latex.additionalBib": [

    ],
    "latex-workshop.chktex.enabled": false,
    "cSpell.words": [
        "bisimilar",
        "bisimilarity",
        "bisimulated",
        "bisimulation",
        "blum's",
        "combinator",
        "commutativity",
        "daniil's",
        "injective",
        "injectivity",
        "ker's",
        "lloc",
        "moivre's",
        "reduct",
        "reducts",
        "regnier",
        "subcase",
        "subterms",
        "surjective",
        "synctex"
    ],
    "cSpell.ignoreRegExpList": [
        "\\\\\\w*(\\[.*?\\])?(\\{.*?\\})?",
        "\\$.+?\\$"
    ],
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ]
}